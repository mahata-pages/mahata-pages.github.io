    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@playwright/experimental-ct-react": "^1.57.0",
    "@playwright/test": "^1.57.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
